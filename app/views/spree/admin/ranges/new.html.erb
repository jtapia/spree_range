<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @range } %>

<% content_for :page_title do %>
  <%= Spree.t(:new_range) %>
<% end %>

<%= form_for [:admin, @range], html: { multipart: true } do |f| %>
  <fieldset data-hook='new_range'>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>